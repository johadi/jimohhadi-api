{
  "name": "jimohhadi",
  "version": "1.0.0",
  "description": "API for my personal website",
  "main": "index.js",
  "scripts": {
    "start": "node app.compile.js",
    "clean-dist": "rm -rf ./dist && mkdir -p ./dist",
    "copy-to-dist": "cp -r ./public/* ./dist/",
    "set-dist": "npm run clean-dist && npm run copy-to-dist",
    "build:prod": "export NODE_ENV=production && npm run set-dist && webpack --mode production && node app.compile.js",
    "build:dev": "npm run set-dist && webpack-dev-server --config ./webpack.config.dev.js --mode development",
    "heroku-prebuild": "npm run set-dist",
    "heroku-postbuild": "webpack --mode production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "jimoh hadi",
  "license": "ISC",
  "engines": {
    "node": "9.11.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.5",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "css-loader": "^0.28.11",
    "dotenv": "^6.0.0",
    "express": "^4.17.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.10",
    "node-sass": "^4.9.0",
    "nodemailer": "^4.6.8",
    "nodemailer-express-handlebars": "^3.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-sweetalert": "0.0.1",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "serve-favicon": "^2.5.0",
    "style-loader": "^0.19.1",
    "validatorjs": "^3.14.2",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  }
}
